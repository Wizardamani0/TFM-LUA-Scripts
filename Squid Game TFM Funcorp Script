function eventG5PlayersNumber()--@G5PATW
G5Players = 0
G5PE = 0
G5PC = 1
for name in next, tfm.get.room.playerList do
if ((tfm.get.room.playerList[name].isDead) == false) then
G5Players = G5Players + 1
end
if G5Players == 2 then
G5PC = G5PC + 1
G5Players = 0
end
end
tfm.exec.setUIMapName(MN.." "..G5MN.." Mice Winners: <ch>"..G5PE.."<n>/<ch>"..G5PC)
print(G5PC)
end--@end

function eventPlayerWon(name)
if MAP == 5 then
G5PE = G5PE + 1
tfm.exec.setUIMapName(MN.." "..G5MN.." Mice Winners: <ch>"..G5PE.."<n>/<ch>"..G5PC)
if G5PC == G5PE then
G5RT = 2
tfm.exec.setGameTime(5)
end
end
end

function eventWinnerMap()
tfm.exec.newGame([[@7931537]])
tfm.exec.setGameTime(13)
tfm.exec.setUIMapName(MN.." <j>The Winner is <rose>"..Winner)
tfm.exec.movePlayer(Winner,400,200)
ui.removeTextArea(0)
MAP = -1
end

