function eventKeyboard(name, key, down, x, y)
if key == 70 then
tfm.exec.addPhysicObject(3,x-45,y+10,{
type=19,
restitution=0.2,
friction=0.3,
width=20,
height=20,
groundCollision=true
})
t1 = tfm.exec.addImage("17b6004c6db.png", "!1", x-65, y-10, nil)
end 
if key == 70 then
ui.removeTextArea(120)
tfm.exec.bindKeyboard(name, 70, true, false)
end
end
